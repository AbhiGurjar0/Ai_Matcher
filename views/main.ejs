    <script>
        const models = ["ChatGPT", "Gemini", "Claude"];
        const searchInput = document.getElementById("searchInput");
        const suggestionsBox = document.getElementById("suggestions");
        const modelsContainer = document.getElementById("modelsContainer");

        function showSuggestions() {
            const query = searchInput.value.toLowerCase();
            suggestionsBox.innerHTML = "";
            if (query === "") {
                suggestionsBox.classList.add("hidden");
                return;
            }
            const filtered = models.filter(model => model.toLowerCase().includes(query));
            if (filtered.length === 0) {
                suggestionsBox.classList.add("hidden");
                return;
            }
            suggestionsBox.classList.remove("hidden");
            filtered.forEach(model => {
                const li = document.createElement("li");
                li.className = "p-2 hover:bg-gray-700 cursor-pointer";
                li.innerText = model;
                li.onclick = () => {
                    searchInput.value = model;
                    suggestionsBox.classList.add("hidden");
                    performSearch();
                };
                suggestionsBox.appendChild(li);
            });
        }

        function performSearch() {
            const query = searchInput.value.toLowerCase();
            document.querySelectorAll(".ai-model").forEach(card => {
                const name = card.getAttribute("data-name").toLowerCase();
                card.style.display = name.includes(query) ? "flex" : "none";
            });
        }

        document.addEventListener("click", (event) => {
            if (!event.target.closest("#suggestions") && event.target !== searchInput) {
                suggestionsBox.classList.add("hidden");
            }
        });
    </script>
